{
  "version": 2,
  "name": "gclaude-enterprise-system",
  "builds": [
    {
      "src": "enterprise-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/enterprise-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "enterprise-server.js": {
      "maxDuration": 30
    }
  }
}